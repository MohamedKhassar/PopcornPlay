{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(googlebot|bingbot|yahoo|baiduspider|facebookexternalhit|twitterbot|linkedinbot)"
        }
      ],
      "destination": "https://service.prerender.io/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Prerender-Token",
          "value": "RwnDkleVcQ532rIB9Fb6"
        }
      ]
    }
  ]
}
